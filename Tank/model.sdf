<?xml version="1.0"?>
<sdf version="1.6">
  <model name="my_tank">
    <static>false</static>

    <!-- Main Body -->
    <link name="body">
      <pose>0 0 0.5 0 0 0</pose> <!-- Adjust pose as needed -->
      <collision name="body_collision">
        <geometry>
          <mesh>
            <uri>model://my_tank/meshes/Body.stl</uri>
            <scale>1 1 1</scale>
          </mesh>
        </geometry>
      </collision>
      <visual name="body_visual">
        <geometry>
          <mesh>
            <uri>model://my_tank/meshes/Body.stl</uri>
            <scale>1 1 1</scale>
          </mesh>
        </geometry>
        <material>
          <ambient>0.5 0.5 0.5 1</ambient>
          <diffuse>0.8 0.8 0.8 1</diffuse>
          <specular>0.1 0.1 0.1 1</specular>
        </material>
      </visual>
      <inertial>
        <mass>100</mass> <!-- Adjust mass as needed -->
        <inertia>
          <ixx>1.0</ixx>
          <iyy>1.0</iyy>
          <izz>1.0</izz>
        </inertia>
      </inertial>
    </link>

    <!-- Cannon -->
    <link name="cannon">
      <pose>0.5 0 0.8 0 0 0</pose> <!-- Adjust pose as needed -->
      <collision name="cannon_collision">
        <geometry>
          <mesh>
            <uri>model://my_tank/meshes/Cannon.stl</uri>
            <scale>1 1 1</scale>
          </mesh>
        </geometry>
      </collision>
      <visual name="cannon_visual">
        <geometry>
          <mesh>
            <uri>model://my_tank/meshes/Cannon.stl</uri>
            <scale>1 1 1</scale>
          </mesh>
        </geometry>
        <material>
          <ambient>0.4 0.4 0.4 1</ambient>
          <diffuse>0.7 0.7 0.7 1</diffuse>
          <specular>0.1 0.1 0.1 1</specular>
        </material>
      </visual>
      <inertial>
        <mass>20</mass> <!-- Adjust mass as needed -->
        <inertia>
          <ixx>0.1</ixx>
          <iyy>0.1</iyy>
          <izz>0.1</izz>
        </inertia>
      </inertial>
    </link>

    <!-- Left Skirt -->
    <link name="skirt_left">
      <pose>-0.4 0.3 0.2 0 0 0</pose> <!-- Adjust pose as needed -->
      <collision name="skirt_left_collision">
        <geometry>
          <mesh>
            <uri>model://my_tank/meshes/Skirt_L.stl</uri>
            <scale>1 1 1</scale>
          </mesh>
        </geometry>
      </collision>
      <visual name="skirt_left_visual">
        <geometry>
          <mesh>
            <uri>model://my_tank/meshes/Skirt_L.stl</uri>
            <scale>1 1 1</scale>
          </mesh>
        </geometry>
        <material>
          <ambient>0.5 0.5 0.5 1</ambient>
          <diffuse>0.8 0.8 0.8 1</diffuse>
          <specular>0.1 0.1 0.1 1</specular>
        </material>
      </visual>
      <inertial>
        <mass>10</mass> <!-- Adjust mass as needed -->
        <inertia>
          <ixx>0.05</ixx>
          <iyy>0.05</iyy>
          <izz>0.05</izz>
        </inertia>
      </inertial>
    </link>

    <!-- Right Skirt -->
    <link name="skirt_right">
      <pose>-0.4 -0.3 0.2 0 0 0</pose> <!-- Adjust pose as needed -->
      <collision name="skirt_right_collision">
        <geometry>
          <mesh>
            <uri>model://my_tank/meshes/Skirt_R.stl</uri>
            <scale>1 1 1</scale>
          </mesh>
        </geometry>
      </collision>
      <visual name="skirt_right_visual">
        <geometry>
          <mesh>
            <uri>model://my_tank/meshes/Skirt_R.stl</uri>
            <scale>1 1 1</scale>
          </mesh>
        </geometry>
        <material>
          <ambient>0.5 0.5 0.5 1</ambient>
          <diffuse>0.8 0.8 0.8 1</diffuse>
          <specular>0.1 0.1 0.1 1</specular>
        </material>
      </visual>
      <inertial>
        <mass>10</mass> <!-- Adjust mass as needed -->
        <inertia>
          <ixx>0.05</ixx>
          <iyy>0.05</iyy>
          <izz>0.05</izz>
        </inertia>
      </inertial>
    </link>

    <!-- Turret -->
    <link name="turret">
      <pose>0 0 0.8 0 0 0</pose> <!-- Adjust pose as needed -->
      <collision name="turret_collision">
        <geometry>
          <mesh>
            <uri>model://my_tank/meshes/Turret.stl</uri>
            <scale>1 1 1</scale>
          </mesh>
        </geometry>
      </collision>
      <visual name="turret_visual">
        <geometry>
          <mesh>
            <uri>model://my_tank/meshes/Turret.stl</uri>
            <scale>1 1 1</scale>
          </mesh>
        </geometry>
        <material>
          <ambient>0.5 0.5 0.5 1</ambient>
          <diffuse>0.8 0.8 0.8 1</diffuse>
          <specular>0.1 0.1 0.1 1</specular>
        </material>
      </visual>
      <inertial>
        <mass>30</mass> <!-- Adjust mass as needed -->
        <inertia>
          <ixx>0.2</ixx>
          <iyy>0.2</iyy>
          <izz>0.2</izz>
        </inertia>
      </inertial>
    </link>

    <!-- Left Wheel -->
    <link name="wheel_left">
      <pose>-0.5 0.4 0.1 0 0 0</pose> <!-- Adjust pose as needed -->
      <collision name="wheel_left_collision">
        <geometry>
          <mesh>
            <uri>model://my_tank/meshes/Wheel_L.stl</uri>
            <scale>1 1 1</scale>
          </mesh>
        </geometry>
      </collision>
      <visual name="wheel_left_visual">
        <geometry>
          <mesh>
            <uri>model://my_tank/meshes/Wheel_L.stl</uri>
            <scale>1 1 1</scale>
          </mesh>
        </geometry>
        <material>
          <ambient>0.3 0.3 0.3 1</ambient>
          <diffuse>0.6 0.6 0.6 1</diffuse>
          <specular>0.1 0.1 0.1 1</specular>
        </material>
      </visual>
      <inertial>
        <mass>5</mass> <!-- Adjust mass as needed -->
        <inertia>
          <ixx>0.01</ixx>
          <iyy>0.01</iyy>
          <izz>0.01</izz>
        </inertia>
      </inertial>
    </link>

    <!-- Right Wheel -->
    <link name="wheel_right">
      <pose>-0.5 -0.4 0.1 0 0 0</pose> <!-- Adjust pose as needed -->
      <collision name="wheel_right_collision">
        <geometry>
          <mesh>
            <uri>model://my_tank/meshes/Wheel_R.stl</uri>
            <scale>1 1 1</scale>
          </mesh>
        </geometry>
      </collision>
      <visual name="wheel_right_visual">
        <geometry>
          <mesh>
            <uri>model://my_tank/meshes/Wheel_R.stl</uri>
            <scale>1 1 1</scale>
          </mesh>
        </geometry>
        <material>
          <ambient>0.3 0.3 0.3 1</ambient>
          <diffuse>0.6 0.6 0.6 1</diffuse>
          <specular>0.1 0.1 0.1 1</specular>
        </material>
      </visual>
      <inertial>
        <mass>5</mass> <!-- Adjust mass as needed -->
        <inertia>
          <ixx>0.01</ixx>
          <iyy>0.01</iyy>
          <izz>0.01</izz>
        </inertia>
      </inertial>
    </link>

    <!-- Joints -->
    <!-- Turret Joint -->
    <joint name="turret_joint" type="revolute">
      <parent>body</parent>
      <child>turret</child>
      <axis>
        <xyz>0 0 1</xyz>
        <limit>
          <lower>-1.57</lower> <!-- -90 degrees -->
          <upper>1.57</upper>  <!-- +90 degrees -->
        </limit>
      </axis>
    </joint>

    <!-- Cannon Joint -->
    <joint name="cannon_joint" type="revolute">
      <parent>turret</parent>
      <child>cannon</child>
      <axis>
        <xyz>0 1 0</xyz>
        <limit>
          <lower>-0.5</lower> <!-- -30 degrees -->
          <upper>0.5</upper>  <!-- +30 degrees -->
        </limit>
      </axis>
    </joint>

    <!-- Wheel Joints -->
    <joint name="wheel_left_joint" type="revolute">
      <parent>body</parent>
      <child>wheel_left</child>
      <axis>
        <xyz>0 1 0</xyz>
      </axis>
    </joint>

    <joint name="wheel_right_joint" type="revolute">
      <parent>body</parent>
      <child>wheel_right</child>
      <axis>
        <xyz>0 1 0</xyz>
      </axis>
    </joint>

    <!-- Add more joints as needed -->
  </model>
</sdf>
